  <div class="app_register">
    <div class="app_register_container">
        <h1 id="logo">Register Here!</h1>
    <form class="register_form" action="/register" method="POST">

      <div class="register_form_input">
        <h2>UserName</h2>
        <input type="text" id="username" placeholder="Username" name="username">
      </div>
      <div class="register_form_input">
        <h2>Year</h2>
        <select id="inputState" class="form-select">
          <option selected>Choose</option>
          <option value="First Year">First Year</option>
          <option value="Second Year">Second Year</option>
          <option value="Third Year">Third Year</option>
          <option value="Fourth Year">Fourth Year</option>
          <option value="passout Year">passout</option>
        </select>
      </div>
      <div class="register_form_input">
        <h2>Skills</h2>
        <input type="text" id="username" placeholder="Enter Skill" name="skils">
      </div>
      <div class="register_form_input">
        <h2 for="password">Password</h2>
        <input type="password" id="password" name="password">
      </div>
      <div class="register_form_input">
        <h2>Confirm Password</h2>
        <input type="password" id="passwordConfirm" name="passwordConfirm">
      </div>
      <div class="register_form_input">
        <h2>Mobile number</h2>
        <input type="text" placeholder="Mobile number" name="phone" id="phone">
      </div>
      <div class="register_form_input">
        <h2>Email</h2>
        <input type="email"          placeholder="Enter your email" name="email">
      </div>
        <small id="emailhelp" class="form-text text-muted"> We'll never share your email with anyone else</small>
      <button type="submit"  class="btn-style btn-primary register_form_button">submit</button>
    </form>
    <p class="register_form_login_button_extra" >
        Already Registered. 
        <a class="register_form_login_button" href="/login" >Login Here</a>
        </p>
     
    </div>
  </div>

  <script src="../jq/jquery.js" type="text/javascript"></script>
